---
import Logo from "./Logo.astro";

const MENU = [
	{
		label: "Oferta",
		link: "#oferta",
	},
	{
		label: "Dlaczego my?",
		link: "#dlaczego-my",
	},
	{
		label: "Kontakt",
		link: "#kontakt",
	},
];

const { theme } = Astro.props;
---

<nav>
	<Logo type={theme} />
	<ul>
		{
			MENU.map(({ label, link }) => (
				<li>
					<a href={link}>{label}</a>
				</li>
			))
		}
	</ul>
</nav>
